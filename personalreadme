# Convert command for ripped images
for i in *.png;     do convert "$i" -modulate 100,105,183 -fx '(1/(1+exp(10*(.5-u)))-0.006693)*1.013567'  -morphology Erode Octagon:1 -crop 100%x+0+3 "${i%.*}.png";  done

# import scales into mongo
mongoimport --db scales --collection scales --drop --file seeds/scales.json --jsonArray
